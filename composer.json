{
    "name": "komodohq/siesta",
    "description": "Traits mixing for consuming REST APIs",
    "homepage": "http://github.com/KomodoHQ/Siesta",
    "keywords": ["REST", "API"],
    "license": "MIT",
    "authors": [
        {
            "name": "Will McKenzie",
            "email": "will@komododigital.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "2.4.*@stable",
        "mockery/mockery": "*@stable"
    },
    "autoload": {
        "psr-4": {
            "Siesta\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin/"
    }
}
